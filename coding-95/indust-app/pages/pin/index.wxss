/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #484848;
}

.btn {
  background: #42b983;
  color: #fff;
}
.other-button-hover {
  background-color: #42df83;
}

.header {
  display: block;
  text-align: center;
  padding: 60rpx 0;
  color: #484848;
  font-size: 40rpx;
}

.title {
  display: block;
  text-align: center;
  padding: 40rpx 0;
  color: #484848;
  font-size: 40rpx;
}
